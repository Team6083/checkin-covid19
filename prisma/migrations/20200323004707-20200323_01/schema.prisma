datasource sqlite {
  url = "***"
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  studentNumber String       @id
  cardUUID      String
  name          String
  checkinLogs   CheckinLog[]
}

model CheckinLog {
  id          Int      @id
  user        User     @relation(references: [studentNumber])
  checkinAt   DateTime
  temperature Float
}

model AdminAccounts {
  id           Int    @id
  userName     String @unique
  passwordHash String
  salt         String
}