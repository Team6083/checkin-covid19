datasource sqlite {
  url = "***"
  provider = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  studentNumber String       @id
  cardUUID      String?
  name          String
  checkinLogs   CheckinLog[]
}

model CheckinLog {
  id          Int      @default(autoincrement()) @id
  user        User     @relation(references: [studentNumber])
  checkinAt   DateTime
  temperature Float
}

model AdminAccounts {
  id           Int     @default(autoincrement()) @id
  userName     String  @unique
  passwordHash String
  salt         String
  tokens       Token[]
}

model Token {
  jti    Int           @default(autoincrement()) @id
  iss    AdminAccounts @relation(references: [id])
  iat    DateTime
  exp    DateTime
  scopes String
}